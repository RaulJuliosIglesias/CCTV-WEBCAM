{
  "AppSettings": {
    "CameraName": "RTSP VirtualCam",
    "DefaultResolution": "auto",
    "DefaultFrameRate": 30,
    "Transport": "tcp",
    "TimeoutSeconds": 30,
    "AutoReconnect": true,
    "StartMinimized": false,
    "StartWithWindows": false,
    "RememberLastUrl": true,
    "MaxUrlHistory": 10,
    "Theme": "system"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "File": {
      "Path": "logs/rtspvirtualcam.log",
      "MaxSizeMB": 10,
      "MaxFiles": 5
    }
  }
}
